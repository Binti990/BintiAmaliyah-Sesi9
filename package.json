  "type": "module",
  "description": "Selenium WebDriver tests using Mocha with Mochawesome reports",
  "main": "index.js",
  "scripts": {
    "test": "mocha test/login.js --reporter mocha-simple-html-reporter --reporter-options output=report.html",
    "test1": "mocha test/atoz.js --reporter mocha-simple-html-reporter --reporter-options output=report.html",
    "test2": "mocha test/hook-login.js --reporter mocha-simple-html-reporter --reporter-options output=report.html",
    "test3": "mocha test/hook-atoz.js --reporter mocha-simple-html-reporter --reporter-options output=report.html",
    "test4": "mocha test/sesi10.js --reporter mocha-simple-html-reporter --reporter-options output=report.html",
    "test5": "mocha test/sesi11.js --reporter mocha-simple-html-reporter --reporter-options output=report.html",
    "jalankan-mochawesome": "mocha test/login.js --timeout 60000 --reporter mochawesome --reporter-options reportDir=mochawesome-report",
    "jalankan-mochawesome1": "mocha test/atoz.js --timeout 60000 --reporter mochawesome --reporter-options reportDir=mochawesome-report",
    "jalankan-mochawesome2": "mocha test/hook-login.js --timeout 60000 --reporter mochawesome --reporter-options reportDir=mochawesome-report",
    "jalankan-mochawesome3": "mocha test/hook-atoz.js --timeout 60000 --reporter mochawesome --reporter-options reportDir=mochawesome-report",
    "jalankan-mochawesome4": "mocha test/sesi10.js --timeout 60000 --reporter mochawesome --reporter-options reportDir=mochawesome-report",
    "jalankan-mochawesome5": "mocha test/sesi11.js --timeout 60000 --reporter mochawesome --reporter-options reportDir=mochawesome-report",
    "jalankan-simple-html": "mocha test/hook-sesi10.js --reporter mocha-simple-html-reporter --reporter-options output=report.html"
  },
